{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "JwtSettings": {
    "TokenType": "Bearer",
    "PublicCertificatePathName": "pub.crt",
    "PrivateKeyPathName": "pri.key",
    "JwtTokenSettings": {
      "TokenType": "Bearer",
      "ValidateIssuerSigningKey": true,
      "ValidateLifetime": true,
      "ValidateIssuer": false,
      "ValidIssuer": "",
      "ValidateAudience": false,
      "ValidAudience": "",
      "ClockSkew": 0,
      "Expires": 999999
    }
  }
}
